{
  "name": "@codeparticle/rdx",
  "version": "3.1.0",
  "description": "RDX is a module based redux framework that generates boilerplate for you.",
  "keywords": [
    "rdx",
    "redux",
    "state",
    "generation",
    "react"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Nick Krause <nick@codeparticle.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeparticle/rdx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "eslint --quiet 'src/**/*.ts' 'test/**/*.ts' 'tools/**/*.ts'",
    "lint:fix": "eslint --quiet --fix **/*.ts",
    "lint:files:fix": "eslint --quiet --fix",
    "prebuild": "rimraf dist",
    "build": "tsup & tsup src/index.ts --no-config true --dts=src/index.ts --dts-resolve --clean false & wait",
    "build:watch": "tsup --watch src",
    "tsc:watch": "tsc --watch",
    "start": "pnpm tsc:watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:prod": "pnpm run lint & pnpm run test & wait",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepush": "pnpm run test:prod && pnpm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "release": "standard-version",
    "postrelease": "pnpm release:github && pnpm release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "pnpm publish --access public"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --quiet --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "@swc/jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "~16.2.1",
    "@commitlint/config-conventional": "~16.2.1",
    "@redux-devtools/extension": "~3.2.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "~1.2.141",
    "@swc/helpers": "^0.3.3",
    "@swc/jest": "~0.2.17",
    "@types/jest": "~27.4.0",
    "@types/node": "~17.0.18",
    "@typescript-eslint/eslint-plugin": "~5.12.0",
    "@typescript-eslint/parser": "~5.12.0",
    "colors": "~1.4.0",
    "commitizen": "~4.2.4",
    "coveralls": "~3.1.1",
    "cross-env": "~7.0.3",
    "cz-conventional-changelog": "~3.3.0",
    "esbuild": "^0.14.22",
    "eslint": "~8.9.0",
    "eslint-config-standard-with-typescript": "~21.0.1",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~6.0.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "jest-config": "~27.5.1",
    "lint-staged": "~12.3.4",
    "prompt": "~1.2.2",
    "redux": "~4.1.2",
    "redux-saga": "~1.1.3",
    "replace-in-file": "~6.3.2",
    "rimraf": "~3.0.2",
    "semantic-release": "~19.0.2",
    "shelljs": "~0.8.5",
    "standard-version": "~9.3.2",
    "travis-deploy-once": "~5.0.11",
    "ts-jest": "~27.1.3",
    "ts-node": "^10.5.0",
    "ts-toolbelt": "~9.6.0",
    "tslib": "~2.3.1",
    "tsup": "^5.11.13",
    "type-fest": "~2.12.0",
    "typedoc": "~0.22.11",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "change-case": "~4.1.2"
  },
  "peerDependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "redux": ">=4.0.0",
    "redux-saga": "^1.1.3"
  }
}
